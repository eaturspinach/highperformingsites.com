%h2
  Dashboard
%section#micropost.module
  %h2
    Micropost
  = render 'shared/micropost_form'
  = render 'shared/feed'
%section#beacon-data.module
  %h2
    URLs
  %table#dashboard
    %thead
      %th
        URL
      %th
        Latest Recorded Load Time
      %th
        Latest Recorded Date
    %tbody
      - @urls.each do |u|
        %tr{:class=>"#{cycle('odd', 'even')} row"}
          %td.url      
            = link_to u.name, u
          %td.load_time
            = (u.yslow2s.find(:last) == nil) ? 'N/A' : (grade(u.yslow2s.find(:last).lt))
          %td.date
            = (u.yslow2s.find(:last) == nil) ? 'N/A' : u.yslow2s.find(:last).created_at.to_s(:short)
